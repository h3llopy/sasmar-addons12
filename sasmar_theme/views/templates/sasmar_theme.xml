<!-- Most Popular Products -->
<template id ="most_popular_products">
						
<h2 class="sidebar-title">Most Popular Products</h2>
<div class="sidebar-filterproducts custom-block" style="padding-bottom: 20px;">
	<div id="featured_product" class="owl-top-narrow">
				<div class="products wrapper grid side-list products-grid">
				<div class="filterproducts products list items product-items owl-carousel" style="display: block!important;">
												<div class="item">
							<t t-set="website_product" t-value="website.get_popular_products()"/>
								<t t-foreach="website_product" t-as="product">

								<div class="product product-item" style="border: 1px solid #ddd; height: auto !important;">
				<div class="product-item-info" data-container="product-grid">
										<a t-attf-href="/shop/product/#{slug(product)}" tabindex="-1">
													<img class="product-image-photo default_image" t-att-src="website.image_url(product, 'image', None if product_image_big else '300x300')" alt="" style="height:200px; width:200px;"/>
																														</a>
					<div class="product details product-item-details">
					
					
								   
					   <div class="product-reviews-summary short">
							<div class="rating-summary">
								  <!--Price-->
					<em class="p-price" itemprop="offers" itemscope="itemscope" itemtype="http://schema.org/Offer" t-if="product.product_variant_ids">
				   <span class="price-label"></span>
				   <span class="price" id="product-price-3-widget-catalogsale-daadc6e791f3d16a73c4d30261ff95e9">
				   <t t-if="(product.lst_price - product.price) &gt; 0.1">

					 <del class="text-danger" style="white-space: nowrap;" t-field="product.lst_price" t-field-options='{
							"widget": "monetary",
							"from_currency": "product.company_id.currency_id",
							"display_currency": "website.currency_id"
							}'/>&amp;nbsp;
				 </t>
				 </span>

				 <span class="price-label"></span>
				<span class="price" id="product-price-3-widget-catalogsale-daadc6e791f3d16a73c4d30261ff95e9">
				 <span class="regular-price" id="product-price-48-new">
															  <span class="price" t-field="product.price" style="white-space: nowrap;"  t-field-options='{
																   "widget": "monetary",
																   "display_currency": "website.currency_id"
															   }'>
															  </span></span>
															  <span itemprop="priceCurrency" style="display:none;" t-esc="user_id.partner_id.property_product_pricelist.currency_id.name"/>
															  <span itemprop="price" style="display:none;" t-esc="product.price"/>
				</span>

					</em>
					<!--End Price--> 
							</div>
						</div>       
						
						
												<strong class="product name product-item-name">
							<a class="product-item-link"
							   t-attf-href="/shop/product/#{slug(product)}"> <span t-field="product.name"/></a>
						</strong>
				   
																   
																						 
 
				   </div>
				</div>
				</div>
				</t>
								</div>
									</div>
	</div>

	</div>

</div>

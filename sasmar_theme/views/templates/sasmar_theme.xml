<odoo>
<data>

	<template id="sasmar_categories_recursive" name="Sasmar Category list">
		<li class="dropdown dropdown-submenu ayaz">
			<!-- <span t-field="c.name"/> -->
			<a t-attf-href="/shop/category/#{slug(c)}" t-attf-class="dropdown-toggle nav-link">
				<span t-field="c.name"/>
			</a>
			<ul t-if="c.child_id" class="dropdown-menu ayaz2">
				<t t-foreach="c.child_id" t-as="c">
					<t t-call="sasmar_theme.sasmar_categories_recursive" />
				</t>
			</ul>
		</li>
	</template>

<!-- Customize Header -->
<template id="homepage_top_header" inherit_id="website.layout">
	<xpath expr="//header//nav[hasclass('navbar-expand-md')]" position="before">
		<div class="panel wrapper hdr sasmar-wrapper">
			<div class="panel header sasmar-panelheader pull-right" style="margin-right: 8%;">
				<div id="currency" >
					<div class="dropdown btn-group">
						<a href="#" class="dropdown-toggle sasmar-toggle" data-toggle="dropdown">
							<img src="/sasmar_theme/static/src/img/flags/us.gif" />
							<span>USD-US Dollar</span>
							<span class="caret"></span>
						</a>
						<ul class="dropdown-menu" role="menu">
							<li>
								<a href="#">
									<img src="/sasmar_theme/static/src/img/flags/us.gif" />
									<span>EUR-Euro</span>
								</a>
							</li>
						</ul>
					</div>
				</div>
				
				
				<span>
				<span class="split"></span>
					<a class="sasmar-sample" href="/sample">
						<span class="menu-text"> Free Sample</span>
					</a>
				</span>
			</div>
		</div>

		<div class="panel wrapper hdr sasmar-wrapper2">
			<div class="panel header sasmar-panelheader2">
				<div data-block="minicart" class="minicart-wrapper header-account">
					<div class="custom-block">
						<span t-if="website.user_id != user_id">
						<t t-set="website_sale_order" t-value="website.sale_get_order()" />
						<a href="/my/home" class="sasmar-account">
							<i class="fa fa-user"></i>
							<span class="menu-text"> Account</span>
						</a>
						<span class="split"></span>
						</span>
						<span
							t-if="website_sale_order and website_sale_order.website_order_line">
							<a href="/shop/cart" class="sasmar-account">
								<i class="fa fa-check-square-o"></i>
								<span class="menu-text"> Checkout</span>
							</a>
						</span>

						<span t-if="website.user_id == user_id">
							<a href="/web/login" class="sasmar-account">
								<i class="fa fa-key"></i>
								<span class="menu-text"> Login</span>
							</a>
							<span class="split"></span>
							<a href="/web/signup" class="sasmar-account">
								<i class="fa fa-sign-in"></i>
								<span class="menu-text"> Signup</span>
							</a>
						</span>
						<span t-if="website.user_id != user_id">
							<a href="/web/session/logout?redirect=/" class="sasmar-account">
								<i class="fa fa-unlock"></i>
								<span class="menu-text"> Logout</span>
							</a>
						</span>
					</div>
					<div class="cart-toggle sasmar-cart">&#160;
						<a class="sasmar-minicart" href="/shop/cart">
							<div class="icon">
								<i class="fa fa-shopping-cart"></i>
							</div>
						</a>
						<t t-set="website_sale_order" t-value="website.sale_get_order()" />
						<a class="sasmar-minicart" href="/shop/cart">
							<div class="mini-cart">
								<span class="count-item">
									<span class="txt-item my_cart_quantity"
										t-esc="website_sale_order and website_sale_order.cart_quantity or 0"></span>
								</span>
							</div>
						</a>
						
						<div class="top-cart-content">
							<div
								t-if="not website_sale_order or not website_sale_order.website_order_line"
								class="">
								Your cart is empty!
							</div>
							<ol class="mini-products-list"
								t-if="website_sale_order and website_sale_order.website_order_line">
								<t t-foreach="website_sale_order.website_order_line" t-as="line">
									<li class="item last odd"
										t-if="line.product_id and line.product_id.product_tmpl_id">
										<a class="product-image sasmar-minicart" title=""
											t-attf-href="/shop/product/#{ slug(line.product_id.product_tmpl_id) }">
											<span t-field="line.product_id.image_small"
												t-field-options='{"widget": "image", "class": "img-rounded"}' />
										</a>
										<div class="product-details">
											<a t-attf-href="#"
												class="btn-remove sasmar-minicart js_add_cart_json_new" title="Remove Item"
												data-no-instant="">
												<i class="fa fa-trash-o"></i>
											</a>
											<a class="btn-edit sasmar-minicart" title="Edit item"
												t-attf-href="/shop/cart">
												<i class="fa fa-pencil"></i>
											</a>
											<p class="product-name">
												<a class="sasmar-minicart"
													t-attf-href="/shop/product/#{ slug(line.product_id.product_tmpl_id) }">
													<span
														t-esc="line.product_id.with_context(display_default_code=False).display_name" />
												</a>
											</p>
											<strong>
												<input type="label" class="js_quantity sasmar-quantity"
													t-att-data-line-id="line.id" t-att-data-product-id="line.product_id.id"
													t-att-value="int(line.product_uom_qty)"
													onchange="alert('hi');" readonly="True" />
											</strong>
											x
											<span
												t-field-options='{
												 "widget": "monetary",
												 "display_currency": "website.currency_id"
											 }'
												t-field="line.price_unit" class="sasmar-priceunit" />
										</div>
									</li>
								</t>
							</ol>
							<div class="top-subtotal"
								t-if="website_sale_order and website_sale_order.amount_total and user_id.partner_id.property_product_pricelist.currency_id">
								Subtotal:
								<span class="price sasmar-priceunit" t-field="website_sale_order.amount_total"
									t-field-options='{
								 "widget": "monetary",
								 "display_currency": "website.currency_id"
									}' />
							</div>
							<div class="actions">
								<t t-set="website_sale_order" t-value="website.sale_get_order()" />
								<a class="sasmar-minicart" t-attf-href="/shop/checkout"
									t-if="website_sale_order and website_sale_order.website_order_line">
									<button class="btn btn-primary" title="Checkout"
										type="button">
										<span>
											<span>Checkout</span>
										</span>
									</button>
								</a>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</xpath>

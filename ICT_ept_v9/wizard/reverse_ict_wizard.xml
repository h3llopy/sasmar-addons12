<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>		
	<record id="reverse_ict_wizard_form_view_ept" model="ir.ui.view">
		<field name="name">Reverse ICT</field>
	    <field name="model">reverse.ict.ept</field>
		<field name="type">form</field>
		<field name="arch" type="xml">
			<form string="Reverse ICT">
			 	<group colspan="4">
			 		<field name='ict_id' readonly="1"/>
			 		<field name="reverse_line_ids">
			 			<tree editable="top">
			 				<field name='product_id'/>
			 				<field name='quantity'/>
			 				<field name='price' />
			 			</tree>
			 		</field>
			 	</group>
			 	<footer><button class="oe_highlight" string="Create Reverse ICT" name="create_reverse_ict" type="object"/><button class="oe_highlight" special="cancel" string="Cancel"/></footer>
        	</form>
		</field>
	</record>
</data>
</openerp>